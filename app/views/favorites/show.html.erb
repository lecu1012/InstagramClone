<div class="row">
  <div class="col-1"></div>
  <div class="col-10 row">
    <% if @favorite == nil %>
      <h3>お気に入りがありません。</h3>
    <% else %>
      <% @favorite.reverse_each do |insta| %>
        <div class="col-md-4 col-sm-12 row-eq-height">
        <%= link_to instagram_path(insta.instagram.id) do %>
        <%= image_tag insta.instagram.image_url , style: 'height:auto;width:100%;'%>
        <% end %>
      </div>
      <% end %>
    <% end %>
  </div>
  <div class="col-1"></div>
</div>
